{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run sass\" \"remix dev\"",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "sass": "sass --watch app/:app/ --load-path=node_modules/radient.style/dist/css --load-path=node_modules/radient.style/scss"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@remix-run/css-bundle": "^1.16.0",
    "@remix-run/node": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@remix-run/serve": "^1.16.0",
    "isbot": "^3.6.8",
    "radient.style": "file:../radient.style/radient.style-0.0.1.tgz",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "prisma": "^4.14.0",
    "sass": "^1.62.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
